<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('./partials/head.ejs')%>
</head>
<body>
  <div class="body-overlay">
    <header>
        <%- include('./partials/header.ejs')%>
    </header>
  <main>
      <div class="container">
            <h2>Rental Car Index</h2>
            <ul>
              <% let listString = renderList(allCars) %>
              <%- listString %>
            </ul>
          </main>
          <footer>
              <%- include('./partials/footer.ejs')%>
          </footer>
          </div>
</body>

<% function renderList(arrList) { %>
<%   let listString = []  %>
<%   arrList.forEach((car, index) => {  %>
<%        listString.push('<li><a href="/cars/' + index + '">' + car.name + '</a></li>') %>
<%    }) %>
<%    return listString.join(" ")  %>
<% } %>